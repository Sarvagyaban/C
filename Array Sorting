#include <stdio.h>

void main() 
{
    int arr[100],i,j,a,n;
    printf("enter the size of array \n");
    scanf("%d",&n);
    printf("enter the elements \n");
    for(i=0; i<n; i++)
    {
        scanf("%d",&arr[i]);
    }    
  for (i = 0; i < n; ++i)
  {
   for (j = i + 1; j < n; ++j)
   {
      if (arr[i] > arr[j])
      {
         a = arr[i];
         arr[i] = arr[j];
         arr[j] = a;
      }
   }
  }
   printf("\nSorted Array \n");
  for(i=0; i<n; i++)
    {
            printf("%d ",arr[i]);
    }
}
