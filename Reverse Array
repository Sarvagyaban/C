#include <stdio.h>

void main() 
{
    int arr[100],i,n;
    printf("enter the size of array \n");
    scanf("%d",&n);
    printf("enter the elements \n");
    for(i=0; i<n; i++)
    {
        scanf("%d",&arr[i]);
    }    
    printf("Reverse Array \n");
    for(i=n-1; i>=0; i--)
    {
        printf("%d ",arr[i]);
    }
}


