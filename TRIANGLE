#include <stdio.h>

int main() 
{
int side1,side2,side3;
     printf("Enter the three sides of Triangle\n");
     scanf("%d", &side1);
     scanf("%d", &side2);
     scanf("%d", &side3);
    if (side1==side2 && side2==side3 && side3==side1)
    printf("Equilateral");
    else 
    if(side1==side2 ||side1==side3||side2==side3 )    
    printf("Isosceles");
    else
    if (side1!=side2 && side2!=side3 && side3!=side1)
    printf("Scalene");
}
